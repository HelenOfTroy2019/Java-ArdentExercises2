import java.util.Scanner;

public class factors {

	public static void main(String[] args) {

		System.out.print("Please enter an integer:");
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		int a = 1;
		
		while (a<num) {
			if (num%a == 0) {
				System.out.print(a+", ");
				a = a + 1;
			}
			
			else {
				a = a + 1;
			}
		}
		
		System.out.print(num);
		
	}

}
